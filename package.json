{
  "name": "team-task-manager",
  "version": "1.0.0",
  "description": "A full-stack team task management application",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:status": "knex migrate:status"
  },
  "keywords": [
    "task",
    "manager",
    "team",
    "react",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.3.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "knex": "^3.0.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "cloudinary": "^1.41.3",
    "concurrently": "^8.2.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.2"
  }
}
